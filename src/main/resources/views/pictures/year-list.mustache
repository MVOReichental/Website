<h1>Fotogalerie</h1>

<div class="row">
	{{#years}}
		<div class="col-sm-6 col-md-4">
			<a href="#/fotogalerie/{{year}}">
				<div class="thumbnail">
					<img ng-src="pictures/{{coverAlbum.id}}/small_{{coverAlbum.cover.file}}.jpg"/>
					<div class="caption">
						<h3>{{year}}</h3>
					</div>
				</div>
			</a>
		</div>
	{{/years}}
</div>