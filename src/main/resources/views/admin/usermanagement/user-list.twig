<table class="table table-striped table-responsive">
    <thead>
        <tr>
            <th>Vorname</th>
            <th>Nachname</th>
            <th>Email-Adresse</th>
            <th>Zuletzt Online</th>
        </tr>
    </thead>

    <tbody>
        {% for user in users if user.enabled == enabled %}
            <tr>
                <td>{{ user.firstName }}</td>
                <td>{{ user.lastName }}</td>
                <td>{{ user.email }}</td>
                <td>{{ user.lastOnline }}</td>
            </tr>
        {% endfor %}
    </tbody>
</table>