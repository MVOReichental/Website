<div class="page-header">
	<h1>Notenverzeichnis - {{title}}</h1>
</div>

<div class="btn-group hidden-print">
	<div class="btn-group">
		<button type="button" class="btn btn-default dropdown-toggle" data-toggle="dropdown">
			Programme <span class="caret"></span>
		</button>
		<ul class="dropdown-menu">
			{{#otherPrograms}}
				<li class="dropdown-header">{{year}}</li>
				{{#programs}}
					<li>
						<a href="intern/notedirectory/programs/{{year}}/{{name}}">{{title}}</a>
					</li>
				{{/programs}}
				<li class="divider"></li>
			{{/otherPrograms}}
		</ul>
	</div>

	<div class="btn-group">
		<button type="button" class="btn btn-default dropdown-toggle" data-toggle="dropdown">
			Kategorien <span class="caret"></span>
		</button>
		<ul class="dropdown-menu">
			{{#categories}}
				<li><a href="intern/notedirectory/categories/{{id}}">{{title}}</a></li>
			{{/categories}}
		</ul>
	</div>

	<a href="intern/notedirectory/titles" class="btn btn-default">Alle Titel</a>
</div>

{{{list}}}